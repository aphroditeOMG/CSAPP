#n brgins at address 0 
        .pos 0
        irmovq stack, %rsp      # Set up stack pointer
        call main               # Execute main program
        halt                    # Terminate program 

# Array of 4 elements
        .align 8
array:  .quad 0x000d000d000d
        .quad 0x00c000c000c0
        .quad 0x0b000b000b00
        .quad 0xa000a000a000

main:   irmovq  array,%rdi
        irmovq  $4,%rsi
        irmovq  $1,%rax
iter:
        subq    %rax,%rsi
        jne     caller2
        irmovq $1, %rax
        irmovq $2, %rdi
        irmovq $4, %rbx
        irmovq $32, %rdx
        jmp     caller2
        irmovq $1, %rsi
        irmovq $2, %rdi
        irmovq $4, %rbp
        irmovq $32, %rax
        nop
        nop
caller2:
        ret

# Stack starts here and grows to lower addresses
        .pos 0x200
stack:


